<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="false" />
        <httpProtocol>
            <customHeaders>
                <clear />
            </customHeaders>
        </httpProtocol>
        <defaultDocument>
            <files>
                <clear />
                <add value="index.asp" />
                <add value="index.php" />
                <add value="Default.htm" />
                <add value="Default.asp" />
                <add value="index.htm" />
                <add value="index.html" />
                <add value="iisstart.htm" />
                <add value="default.aspx" />
            </files>
        </defaultDocument>
        <httpErrors>
            <remove statusCode="500" subStatusCode="-1" />
            <error statusCode="500" subStatusCode="-1" path="500.asp" responseMode="File" />
        </httpErrors>
    </system.webServer>
    <system.web>
        <customErrors mode="Off" />
      <globalization culture="pt-BR" uiCulture="pt-BR" />
      
    </system.web>
</configuration>
